                  <Button fx:id="log_in" mnemonicParsing="false" onAction="#handleButtonAction" onMouseEntered="#loginmousebtnenter" onMouseExited="#loginmousebtnexit" prefHeight="69.0" prefWidth="78.0" style="-fx-background-color: transparent;">
                     <graphic>
                        <ImageView fx:id="login" fitHeight="92.0" fitWidth="156.0" pickOnBounds="true">
                           <image>
                              <Image url="@../image/loginbutton.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="sign_up" mnemonicParsing="false" onAction="#ShangeScreenButtonPushedsingup" onMouseEntered="#singupmousebtnenter" onMouseExited="#singupmousebtnexit" prefHeight="31.0" prefWidth="65.0" style="-fx-background-color: transparent;">
                     <graphic>
                        <ImageView fx:id="signup" fitHeight="85.0" fitWidth="155.0" pickOnBounds="true">
                           <image>
                              <Image url="@../image/singinbutton.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <Hyperlink fx:id="forget" onAction="#forgetpassword" prefHeight="42.0" prefWidth="166.0" text="Forget Passward" textAlignment="CENTER" textFill="#bc9054">
               <font>
                  <Font name="Serif Regular" size="19.0" />
               </font>
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Hyperlink>
         </children>
      </VBox>
      <Button fx:id="home" layoutX="959.0" layoutY="7.0" mnemonicParsing="false" onAction="#ShangeScreenButtonPushed" onMouseEntered="#homemousebtnenter" onMouseExited="#homemousebtnexit" prefHeight="116.0" prefWidth="84.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="101.0" fitWidth="84.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/homeicon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
