<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="670.0" prefWidth="1078.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxfxmlbraino.FXMLloginController">
   <children>
      <ImageView fitHeight="670.0" fitWidth="1078.0" pickOnBounds="true">
         <image>
            <Image url="@../image/backgroundlogin.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="523.0" fitWidth="509.0" layoutX="307.0" layoutY="70.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/loginform.png" />
         </image>
      </ImageView>
      <VBox alignment="CENTER" layoutX="355.0" layoutY="171.0" prefHeight="346.0" prefWidth="396.0" AnchorPane.leftAnchor="355.0" AnchorPane.topAnchor="171.0">
         <children>
            <HBox alignment="BOTTOM_CENTER" prefHeight="63.0" prefWidth="396.0">
               <children>
                  <Label text="User Name ">
                     <font>
                        <Font name="Segoe Print" size="19.0" />
                     </font>
                  </Label>
                  <TextField fx:id="userName" alignment="CENTER" prefHeight="31.0" prefWidth="200.0" promptText="Enter User Name">
                     <font>
                        <Font name="Serif Regular" size="15.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="BOTTOM_CENTER" prefHeight="68.0" prefWidth="376.0">
               <children>
                  <Label prefHeight="21.0" prefWidth="104.0" text="Passward ">
                     <font>
                        <Font name="Segoe Print" size="18.0" />
                     </font>
                  </Label>
                  <PasswordField fx:id="password" alignment="CENTER" prefHeight="31.0" prefWidth="201.0" promptText="Enter Passward">
                     <font>
                        <Font name="Serif Regular" size="15.0" />
                     </font>
                  </PasswordField>
               </children>
               <VBox.margin>
                  <Insets left="15.0" right="5.0" />
               </VBox.margin>
            </HBox>
            <Pane prefHeight="40.0" prefWidth="396.0">
               <children>
                  <Label fx:id="msg" alignment="CENTER" layoutX="4.0" layoutY="-1.0" prefHeight="40.0" prefWidth="396.0" textFill="#eb1717">
                     <font>
                        <Font name="Bell MT" size="18.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <HBox alignment="CENTER" prefHeight="74.0" prefWidth="396.0" spacing="5.0">
